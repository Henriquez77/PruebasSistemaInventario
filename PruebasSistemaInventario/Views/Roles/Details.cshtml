@model PruebasSistemaInventario.Models.Role

@{
    ViewData["Title"] = "Detalles de rol";
}
<div class="container">
<h1>Detalles de rol</h1>

<div>
    <dl class="row">
        <dt class = "col-sm-2">
            @Html.DisplayNameFor(model => model.Nombre)
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.Nombre)
        </dd>
    </dl>
